{
  "webgpt": {
    "name": "webgpt",
    "type": "cli",
    "enabled": true,
    "binary": "npx",
    "scriptPath": "webgpt",
    "workingDirectory": "C:\\scc",
    "timeout": 300000,
    "autoApprove": true,
    "outputFormat": "json",
    "searchProvider": "brave",
    "defaultModel": "gpt-4o",
    "models": [
      "gpt-4o",
      "gpt-4o-mini",
      "gpt-4-turbo"
    ],
    "searchProviders": [
      "brave",
      "google",
      "bing"
    ],
    "features": [
      "web_search",
      "ai_chat",
      "real_time_info"
    ]
  }
}
