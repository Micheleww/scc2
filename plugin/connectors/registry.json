{
  "schema_version": "scc.connector_registry.v1",
  "updated_at": "2026-02-10",
  "connectors": [
    {
      "connector_id": "gateway.local",
      "type": "http_service",
      "endpoints": ["http://127.0.0.1:18788"],
      "scopes": ["board", "dispatch", "replay", "gates", "debug"],
      "allowed_roles": ["*"],
      "audit": { "logs": ["artifacts/executor_logs/route_decisions.jsonl", "artifacts/executor_logs/jobs.jsonl"] }
    },
    {
      "connector_id": "codex.cli",
      "type": "cli",
      "endpoints": ["codex"],
      "scopes": ["llm.chat_completions"],
      "allowed_roles": ["designer", "architect", "integrator", "engineer", "qa", "doc", "retry_orchestrator", "factory_manager"],
      "audit": { "logs": ["artifacts/executor_logs/jobs.jsonl"] }
    },
    {
      "connector_id": "opencode.cli",
      "type": "cli",
      "endpoints": ["opencode"],
      "scopes": ["llm.chat_completions", "llm.providers"],
      "allowed_roles": ["designer", "architect", "integrator", "engineer", "qa", "doc", "pinser", "retry_orchestrator", "factory_manager"],
      "audit": { "logs": ["artifacts/executor_logs/jobs.jsonl"] }
    },
    {
      "connector_id": "webgpt.cli",
      "type": "cli",
      "name": "WebGPT",
      "description": "Web search and AI chat executor with real-time information retrieval",
      "endpoints": ["npx webgpt"],
      "scopes": ["llm.chat_completions", "web.search", "web.browse"],
      "allowed_roles": ["designer", "architect", "integrator", "engineer", "qa", "doc", "researcher", "factory_manager"],
      "config": {
        "search_providers": ["brave", "google", "bing"],
        "default_model": "gpt-4o",
        "timeout_ms": 300000
      },
      "env_vars": ["WEBGPT_API_KEY", "OPENAI_API_KEY", "BRAVE_API_KEY"],
      "audit": { "logs": ["artifacts/executor_logs/webgpt_jobs.jsonl"] }
    },
    {
      "connector_id": "opencode.scc",
      "type": "scc_executor",
      "name": "OpenCode SCC Executor",
      "description": "OpenCode integrated as SCC native executor with multi-model support",
      "endpoints": ["plugin/opencode-scc-executor"],
      "scopes": ["llm.chat_completions", "llm.providers", "code.generation", "code.review"],
      "allowed_roles": ["designer", "architect", "integrator", "engineer", "qa", "doc", "auditor", "verifier_judge", "factory_manager"],
      "config": {
        "binary_path": "C:\\scc\\plugin\\opencode\\opencode.exe",
        "models": ["claude-3.7-sonnet", "claude-3.5-sonnet", "gpt-4o", "gemini-2.5-pro"],
        "default_model": "claude-3.7-sonnet",
        "timeout_ms": 300000,
        "concurrency": 6
      },
      "env_vars": ["ANTHROPIC_API_KEY", "OPENAI_API_KEY", "GEMINI_API_KEY"],
      "audit": { "logs": ["artifacts/executor_logs/opencode_jobs.jsonl"] }
    },
    {
      "connector_id": "openclaw.cli",
      "type": "cli",
      "name": "OpenClaw",
      "description": "Multi-agent orchestration and tool calling executor",
      "endpoints": ["node plugin/openclaw/openclaw.mjs"],
      "scopes": ["llm.chat_completions", "agent.orchestration", "tool.calling", "multi_agent"],
      "allowed_roles": ["designer", "architect", "integrator", "engineer", "qa", "doc", "orchestrator", "factory_manager"],
      "config": {
        "script_path": "C:\\scc\\plugin\\openclaw\\openclaw.mjs",
        "agents": ["default", "coder", "task", "verifier"],
        "default_agent": "default",
        "timeout_ms": 600000,
        "features": ["multi-agent", "tool-calling", "session-management"]
      },
      "env_vars": ["OPENCLAW_API_KEY", "ANTHROPIC_API_KEY", "OPENAI_API_KEY"],
      "audit": { "logs": ["artifacts/executor_logs/openclaw_jobs.jsonl"] }
    }
  ]
}
